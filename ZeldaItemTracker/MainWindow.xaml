<Window x:Class="ZeldaItemTracker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZeldaItemTracker"
        mc:Ignorable="d"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        TextElement.Foreground="{DynamicResource MaterialDesignBody}"
        TextElement.FontWeight="Regular"
        TextElement.FontSize="13"
        TextOptions.TextFormattingMode="Ideal"
        TextOptions.TextRenderingMode="Auto"
        Background="{DynamicResource MaterialDesignPaper}"
        FontFamily="{DynamicResource MaterialDesignFont}"
        ResizeMode="NoResize"
        WindowStyle="None"
        Title="MainWindow" Height="720" Width="633"
        MouseLeftButtonDown="Card_MouseLeftButtonDown">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Dark.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Defaults.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Button.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.PopupBox.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.RichTextBox.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.TextBox.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>

    <Grid>
        <TextBox Name="Woth1" KeyDown="Woth_KeyDown"
            materialDesign:HintAssist.Hint="WOTH 1"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" FontSize="18" HorizontalAlignment="Left" Width="180" Height="44" VerticalAlignment="Top" Margin="21,20,0,0" />
        <TextBox Name="Woth2" KeyDown="Woth_KeyDown"
            materialDesign:HintAssist.Hint="WOTH 2"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="21,85,0,0" FontSize="18" HorizontalAlignment="Left" Width="180" Height="44" VerticalAlignment="Top" RenderTransformOrigin="0.439,0.467" />
        <TextBox Name="Woth3" KeyDown="Woth_KeyDown"
            materialDesign:HintAssist.Hint="WOTH 3"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="21,150,0,0" FontSize="18" HorizontalAlignment="Left" Width="180" Height="44" VerticalAlignment="Top" />
        <TextBox Name="Woth4" KeyDown="Woth_KeyDown"
            materialDesign:HintAssist.Hint="WOTH 4"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="21,215,0,0" FontSize="18" HorizontalAlignment="Left" Width="180" Height="44" VerticalAlignment="Top" />
        <TextBox Name="Woth5" KeyDown="Woth_KeyDown"
            materialDesign:HintAssist.Hint="WOTH 5"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="21,280,0,0" FontSize="18" HorizontalAlignment="Left" Width="180" Height="44" VerticalAlignment="Top" />
        <TextBox
            materialDesign:HintAssist.Hint="Barren 1"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" FontSize="18" HorizontalAlignment="Center" Width="180" Height="44" VerticalAlignment="Top" RenderTransformOrigin="0.611,0.534" Margin="0,20,0,0" />
        <TextBox
            materialDesign:HintAssist.Hint="Barren 2"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="0,85,0,0" FontSize="18" HorizontalAlignment="Center" Width="180" Height="44" VerticalAlignment="Top" />
        <TextBox
            materialDesign:HintAssist.Hint="Barren 3"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="0,150,0,0" FontSize="18" HorizontalAlignment="Center" Width="180" Height="44" VerticalAlignment="Top" />
        <TextBox
            materialDesign:HintAssist.Hint="Skull Mask"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="0,215,0,0" FontSize="18" HorizontalAlignment="Center" Width="180" Height="44" VerticalAlignment="Top" />
        <TextBox
            materialDesign:HintAssist.Hint="Biggoron"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="0,280,0,0" FontSize="18" HorizontalAlignment="Center" Width="180" Height="44" VerticalAlignment="Top" />
        <TextBox
            materialDesign:HintAssist.Hint="30 Skulls"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="434,20,0,0" FontSize="18" HorizontalAlignment="Left" Width="180" Height="44" VerticalAlignment="Top" />
        <TextBox
            materialDesign:HintAssist.Hint="40 Skulls"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="434,85,0,0" FontSize="18" HorizontalAlignment="Left" Width="180" Height="44" VerticalAlignment="Top" />
        <TextBox
            materialDesign:HintAssist.Hint="50 Skulls"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="434,150,0,0" FontSize="18" HorizontalAlignment="Left" Width="180" Height="44" VerticalAlignment="Top" />
        <TextBox
            materialDesign:HintAssist.Hint="OoT"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="434,215,0,0" FontSize="18" HorizontalAlignment="Left" Width="180" Height="44" VerticalAlignment="Top" />
        <TextBox
            materialDesign:HintAssist.Hint="Frogs 2"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="434,280,0,0" FontSize="18" HorizontalAlignment="Left" Width="180" Height="44" VerticalAlignment="Top" />
        <TextBox Name="WothItems" Height="313" HorizontalAlignment="Right" Width="388" VerticalAlignment="Top" Margin="0,352,224,0" FontSize="18" AcceptsTab="True" />
        <TextBox
            materialDesign:HintAssist.Hint="Sometimes 1"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="434,352,0,0" FontSize="18" HorizontalAlignment="Left" Width="180" Height="44" VerticalAlignment="Top" />
        <TextBox
            materialDesign:HintAssist.Hint="Sometimes 2"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="434,426,0,0" FontSize="18" HorizontalAlignment="Left" Width="180" Height="44" VerticalAlignment="Top" />
        <TextBox
            materialDesign:HintAssist.Hint="Sometimes 3"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="434,491,0,0" FontSize="18" HorizontalAlignment="Left" Width="180" Height="44" VerticalAlignment="Top" />
        <TextBox
            materialDesign:HintAssist.Hint="Sometimes 4"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="434,556,0,0" FontSize="18" HorizontalAlignment="Left" Width="180" Height="44" VerticalAlignment="Top" />
        <TextBox
            materialDesign:HintAssist.Hint="Sometimes 5"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="434,621,0,0" FontSize="18" HorizontalAlignment="Left" Width="180" Height="44" VerticalAlignment="Top" />
    </Grid>
</Window>
