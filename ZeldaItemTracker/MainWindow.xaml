<Window x:Class="ZeldaItemTracker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZeldaItemTracker"
        mc:Ignorable="d"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        TextElement.Foreground="{DynamicResource MaterialDesignBody}"
        TextElement.FontWeight="Regular"
        TextElement.FontSize="13"
        TextOptions.TextFormattingMode="Ideal"
        TextOptions.TextRenderingMode="Auto"
        Background="{DynamicResource MaterialDesignPaper}"
        FontFamily="{DynamicResource MaterialDesignFont}"
        ResizeMode="NoResize"
        WindowStyle="None"
        Title="MainWindow" Height="600" Width="633"
        MouseLeftButtonDown="Card_MouseLeftButtonDown">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Dark.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Defaults.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Button.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.PopupBox.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.RichTextBox.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.TextBox.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>

    <Grid>
        <TextBox Name="Woth1" KeyDown="Woth_KeyDown"
            materialDesign:HintAssist.Hint="WOTH 1"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" FontSize="18" HorizontalAlignment="Left" Width="200" Height="44" VerticalAlignment="Top" Margin="10,20,0,0" />
        <TextBox Name="Woth2" KeyDown="Woth_KeyDown"
            materialDesign:HintAssist.Hint="WOTH 2"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="10,85,0,0" FontSize="18" HorizontalAlignment="Left" Width="200" Height="44" VerticalAlignment="Top" RenderTransformOrigin="0.439,0.467" />
        <TextBox Name="Woth3" KeyDown="Woth_KeyDown"
            materialDesign:HintAssist.Hint="WOTH 3"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="10,150,0,0" FontSize="18" HorizontalAlignment="Left" Width="200" Height="44" VerticalAlignment="Top" />
        <TextBox Name="Woth4" KeyDown="Woth_KeyDown"
            materialDesign:HintAssist.Hint="WOTH 4"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="10,215,0,0" FontSize="18" HorizontalAlignment="Left" Width="200" Height="44" VerticalAlignment="Top" />
        <TextBox Name ="OOTItem"
            materialDesign:HintAssist.Hint="OOT Item"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="229,20,0,0" FontSize="18" HorizontalAlignment="Left" Width="200" Height="44" VerticalAlignment="Top" />
        <TextBox Name ="Biggoron"
            materialDesign:HintAssist.Hint="Biggoron"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="229,150,0,0" FontSize="18" HorizontalAlignment="Left" Width="200" Height="44" VerticalAlignment="Top" />
        <TextBox Name ="Skulls30"
            materialDesign:HintAssist.Hint="30 Skulls"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="10,282,0,0" FontSize="18" HorizontalAlignment="Left" Width="200" Height="44" VerticalAlignment="Top" />
        <TextBox Name ="Skulls40"
            materialDesign:HintAssist.Hint="40 Skulls"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="229,282,0,0" FontSize="18" HorizontalAlignment="Left" Width="200" Height="44" VerticalAlignment="Top" />
        <TextBox Name ="Skulls50"
            materialDesign:HintAssist.Hint="50 Skulls"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="443,282,0,0" FontSize="18" HorizontalAlignment="Left" Width="180" Height="44" VerticalAlignment="Top" />
        <TextBox Name ="OOTSong"
            materialDesign:HintAssist.Hint="OOT Song"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="229,85,0,0" FontSize="18" HorizontalAlignment="Left" Width="200" Height="44" VerticalAlignment="Top" />
        <TextBox Name ="Frogs2"
            materialDesign:HintAssist.Hint="Frogs 2"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="229,215,0,0" FontSize="18" HorizontalAlignment="Left" Width="200" Height="44" VerticalAlignment="Top" />
        <TextBox Name="WothItems" Height="229" HorizontalAlignment="Center" Width="589" VerticalAlignment="Top" Margin="0,352,0,0" FontSize="18" AcceptsTab="True" />
        <TextBox Name="Sometimes1"
            materialDesign:HintAssist.Hint="Sometimes 1"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="443,20,0,0" FontSize="18" HorizontalAlignment="Left" Width="180" Height="44" VerticalAlignment="Top" />
        <TextBox Name="Sometimes2"
            materialDesign:HintAssist.Hint="Sometimes 2"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="443,85,0,0" FontSize="18" HorizontalAlignment="Left" Width="180" Height="44" VerticalAlignment="Top" />
        <TextBox Name="Sometimes3"
            materialDesign:HintAssist.Hint="Sometimes 3"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="443,150,0,0" FontSize="18" HorizontalAlignment="Left" Width="180" Height="44" VerticalAlignment="Top" />
        <TextBox Name="Sometimes4"
            materialDesign:HintAssist.Hint="Sometimes 4"
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="443,215,0,0" FontSize="18" HorizontalAlignment="Left" Width="180" Height="44" VerticalAlignment="Top" />
    </Grid>
</Window>
